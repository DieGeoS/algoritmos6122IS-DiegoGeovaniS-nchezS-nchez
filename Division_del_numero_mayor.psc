Algoritmo Division_del_numero_mayor
	Definir N1 Como Real;
	Definir N2 Como Real;
	Definir r Como Real;
	Escribir ("Ingresa un numero");
	Leer N1;
	Escribir ("Ingresa otro Numero");
	Leer N2;
	Si (N1>N2) Entonces
		a<-(N1/N2);
		Escribir ("El resultado es:"), a;
	sino
		a<-(N2/N1);
		Escribir ("El resultado es:"), a;
	FinSi
FinAlgoritmo
